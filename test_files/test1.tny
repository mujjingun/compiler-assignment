int arr[100];
/* binary search routine */

int binarysearch(int x)
{
    int left = 0, right = 100;
    int mid;

    while(left <= right)
    {
        mid = (left + right) / 2;
        if(mid == x) return mid;
        else if(mid < x) left = mid + 1;
        else right = mid - 1;
    }
    return -1;
}

int main(void)
{
    int i, goal, res;
    for(i = 0; i < 100; i++)
    {
        arr[i] = i;
    }
    goal = 100;
    res = binarysearch(goal);
    return 0;
}
